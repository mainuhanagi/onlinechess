/*style for sidebar */
#sidebar{
  position: fixed;
  width: 5%;
  height: 100%;
  background:#f8f7b8; /* #080808|400357|190222*/
  left:0px;
  top:0px;
}

#sidebar.active-menu{
  left:0px;
  width: 10%;
  transition: all 300ms linear;
}
#sidebar .menus{
  position: absolute;
  width: 100%;
  left: 0%;
  box-sizing: border-box;
}
#sidebar.active-menu .menu label{
  position: relative;
  left:50%;
  top:-30px;
  color:#651212;
}

#sidebar .menu{
  display: inline-block;
  color: rgba(255, 255, 255, 0.918);
  list-style: none;
  height: 60px;
  width: 100%;
  border-bottom: 1px solid rgba(243, 140, 6, 0.466);
  box-sizing: border-box;
  margin: 0px 0px;
}

#sidebar .menu:hover{
